{
  "root": "apps/goerp/api-core",
  "sourceRoot": "apps/goerp/api-core",
  "projectType": "application",
  "targets": {
    "hasura": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd apps/goerp/api-core && hasura console --api-port 9602 --console-port 9702"
          }
        ]
      }
    },
    "hasura-seed": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd apps/goerp/api-core && hasura seed apply --database-name ERP"
          }
        ]
      }
    },
    "hasura-update": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "cd apps/goerp/api-core && hasura metadata apply && hasura migrate apply --all-databases && hasura metadata reload"
          }
        ]
      }
    },
    "dcompose": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "docker-compose -f apps/goerp/api-core/docker-compose.yaml up -d"
          }
        ]
      }
    },
    "echo": {
      "executor": "./tools/executors/echo:echo",
      "options": {
        "textToEcho": "Hello World"
      }
    }
  },
  "tags": []
}
